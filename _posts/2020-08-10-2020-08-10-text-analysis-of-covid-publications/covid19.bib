keywords = {Antibodies, Neutralizing,Betacoronavirus,Child,Child, Preschool,Coronavirus Infections,Feces,Female,Humans,Infant,Inhibitory Concentration 50,Male,Nasopharynx,Oropharynx,Pandemics,Pneumonia, Viral,RNA, Viral,Time Factors,Virus Shedding,analysis,biosynthesis,blood,immunology,isolation {\&} purification,virology},
language = {eng},
month = {dec},
number = {1},
