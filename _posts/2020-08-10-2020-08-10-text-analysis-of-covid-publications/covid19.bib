journal = {Emerging microbes {\&} infections},
keywords = {Adolescent,Adult,Aged,Aged, 80 and over,Betacoronavirus,Coronavirus Infections,Female,Germany,Health Personnel,Humans,Male,Middle Aged,Pandemics,Perception,Pneumonia, Viral,Surveys and Questionnaires,Young Adult,epidemiology,physiology,psychology,virology},
language = {eng},
