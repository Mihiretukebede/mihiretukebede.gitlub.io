journal = {International journal of molecular medicine},
language = {eng},
month = {sep},
number = {3},
pages = {957--964},
pmid = {32705153},
title = {{Sensitivity assessment of droplet digital PCR for SARS-CoV-2 detection.}},
volume = {46},
year = {2020}
}
