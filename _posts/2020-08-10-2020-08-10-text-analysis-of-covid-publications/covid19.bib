number = {9},
pages = {2837--2841},
pmid = {32623571},
title = {{The Smallpox Epidemics in America in the 1700s and the Role of the Surgeons: Lessons  to be Learned During the Global Outbreak of COVID-19.}},
volume = {44},
year = {2020}
}
