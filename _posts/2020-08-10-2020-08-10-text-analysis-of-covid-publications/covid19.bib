@article{Li2020b,
abstract = {The implementation effect of the 24-hour Supervise-Correct-Improve (SCI) supervision  model was investigated in COVID-19 isolation ward in putting on and taking off process of personal protective equipment. As shown in results, the error rate of taking off process was significantly reduced (P {\textless} 0.001) by applying the 24h "SCI" mode. Staffs over 40 years old and workers were more likely to make mistakes. Through uninterrupted supervision and protection, application of this mode is proved to be effective.},
