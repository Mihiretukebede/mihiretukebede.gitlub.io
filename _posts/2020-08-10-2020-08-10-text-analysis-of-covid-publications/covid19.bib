pmid = {32652385},
title = {{Drinking to cope with the pandemic: The unique associations of COVID-19-related  perceived threat and psychological distress to drinking behaviors in American men and women.}},
volume = {110},
year = {2020}
}
@article{Shrigiriwar2020,
