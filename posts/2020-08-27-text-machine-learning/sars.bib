journal = {Cmaj},
keywords = {*Personnel, Hospital,*Severe Acute Respiratory Syndrome/complications/drug therapy/physiopathology,Adult,Anti-Bacterial Agents/therapeutic use,Data Collection,Female,Glucocorticoids/therapeutic use,Humans,Male,Methylprednisolone/therapeutic use,Middle Aged,Ontario,Prednisone/therapeutic use,Prospective Studies,Treatment Outcome},
